{
  "version": "0.2.7",
  "license": "MIT",
  "name": "@asany/editor",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "@asany/icons": ">=0.1.11",
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "李茂峰",
  "repository": "https://github.com/limaofeng/asany-editor",
  "module": "dist/editor.esm.js",
  "size-limit": [
    {
      "path": "dist/editor.cjs.production.min.js",
      "limit": "2 MB",
      "ignore": [
        "fonteditor-core"
      ]
    },
    {
      "path": "dist/editor.esm.js",
      "limit": "1 MB",
      "ignore": [
        "fonteditor-core"
      ]
    }
  ],
  "dependencies": {
    "@asany/icons": "^0.1.11",
    "@asany/sortable": "^0.1.22",
    "@daybrush/drag": "^0.19.3",
    "@scena/react-guides": "^0.16.0",
    "moveable-helper": "^0.4.0",
    "overlayscrollbars": "^1.13.1",
    "overlayscrollbars-react": "^0.2.3",
    "react-moveable": "^0.29.0",
    "react-selecto": "^1.12.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.2.4",
    "scenejs": "^1.5.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.4.5",
    "@babel/core": "^7.14.6",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addons": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@types/lodash-es": "^4.14.170",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-is": "^17.0.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-transition-group": "^4.4.1",
    "@types/styled-components": "^5.1.11",
    "antd": "^4.16.10",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "cssnano": "^4.1.11",
    "eslint-plugin-prettier": "^3.4.0",
    "fake-indexeddb": "^3.1.3",
    "graphql": "^15.5.1",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "lodash-es": "^4.17.21",
    "pinst": "^2.1.6",
    "postcss": "^7.0.36",
    "postcss-modules": "^4.1.3",
    "react": "^17.0.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^5.0.1",
    "sunmao": "^0.2.6",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/test/globals-test.ts"
    ]
  }
}
